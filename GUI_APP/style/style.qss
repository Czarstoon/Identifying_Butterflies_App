}

#Model_page QProgressBar {
  text-align: center;
  background-color: #A5D7E8;
  color:#0B2447;
  border-radius: 5px;
}
#Model_page QProgressBar::chunk {
  background-color:#576CBC;
  border-radius: 5px;
}

