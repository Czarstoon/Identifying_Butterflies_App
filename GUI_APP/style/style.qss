/*MainWIndow*/
#MainWindow {
    background-color: #0B2447;
}
/*MenuBar*/
#widget {
    background-color: #19376D;
    border-radius: 5px;
}


#widget QPushButton {
    background-color: #576CBC;
    border-radius: 5px;
    color: #A5D7E8;
    text-align: left;
    padding-left: 10px;
  }

#widget QPushButton:checked {
    background-color: #A5D7E8;
    color:#19376D;
    border: none;

  }
#widget QLabel#title {
    color: #A5D7E8;
}

/*Navigation_page*/
#Navigation_page {
  background-color: #19376D;
  border-radius: 5px;
}

#Navigation_page QLabel{
  color: #A5D7E8;

}

