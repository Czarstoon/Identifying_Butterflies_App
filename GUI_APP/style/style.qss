#Model_page QPushButton {
  color:#19376D;
  border-radius: 5px;
  background-color: #A5D7E8;
  text-align: center;
}

#Model_page QPushButton:disabled {
  background-color: #576CBC;
  color: #A5D7E8;
}
#Model_page QToolTip {
  color:#0B2447;
  
  font-size: 16px; /* rozmiar czcionki ToolTip */
}

#Model_page QGroupBox#groupBox_2 {
  color:#A5D7E8;
